{
  "name": "wa-automation-honda",
  "version": "1.0.0",
  "description": "WhatsApp Automation for Honda Dealer with Fonnte API",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "keywords": [
    "whatsapp",
    "automation",
    "fonnte",
    "honda",
    "dealer"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.34.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^169.0.0",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "openai": "^6.15.0",
    "qrcode-terminal": "^0.12.0",
    "sequelize": "^6.35.0",
    "sharp": "^0.33.0",
    "whatsapp-web.js": "^1.34.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  }
}
